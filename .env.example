# Firebase Configuration
# Substitua pelos valores reais do seu projeto Firebase

# Android Firebase API Key
FIREBASE_API_KEY_ANDROID=your_android_api_key_here

# Web Firebase API Key  
FIREBASE_API_KEY_WEB=your_web_api_key_here

# Firebase Project Configuration
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID_ANDROID=your_android_app_id
FIREBASE_APP_ID_WEB=your_web_app_id
FIREBASE_MEASUREMENT_ID=your_measurement_id
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_DATABASE_URL=https://your_project_id.firebaseio.com
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com

# Android Keystore (para produção)
KEYSTORE_PASSWORD=your_keystore_password
KEY_PASSWORD=your_key_password
KEY_ALIAS=your_key_alias
KEYSTORE_PATH=path/to/your/keystore.jks
